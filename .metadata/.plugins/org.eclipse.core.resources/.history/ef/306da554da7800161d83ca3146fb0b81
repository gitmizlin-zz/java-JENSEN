public class ItemManager {
	
	
	public void createItem(int id, String name, int owner) {
		Item item = new Item(id, name, owner);
		item.setId(id);
		item.setName(name);
		item.setOwner(owner);
	}
	
	public Item getItem(ResultSet rs) throws SQLException {

		return item;
	}
	
		
